{
  "name": "mental-health-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:unit": "jest --testPathPattern=unit.test.ts",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "next": "15.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "papaparse": "^5.4.1",
    "csv-parser": "^3.0.0",
    "recharts": "^2.8.0",
    "lucide-react": "^0.294.0",
    "sonner": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@playwright/test": "^1.40.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "15.0.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/*.test.ts"],
    "collectCoverageFrom": [
      "src/lib/**/*.ts",
      "!src/lib/**/*.d.ts"
    ]
  }
}
